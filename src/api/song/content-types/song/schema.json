      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "artists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::artist.artist",
      "mappedBy": "song"
    },
    "playlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::playlist.playlist",
      "mappedBy": "songs"
    }
  }
